		{
			"name": "Ricerca NR_CDSUO & NR_IDNSP AFFERENZA persona",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "0e66c1d7-a35f-434c-b54e-993e199fca71",
						"exec": [
							"var data = JSON.parse(responseBody);",
							"data.forEach(function(entita){",
							"  if (entita.fineValidita === null && entita.tipoAppartenenza == \"AFFERENZA_UO\") {",
							"    pm.globals.set(\"NR_CDSUO\", entita.entitaOrganizzativa.cdsuo);",
							"    pm.globals.set(\"NR_IDNSIP\", entita.entitaOrganizzativa.idnsip);",
							"    ",
							"    console.log(\"NR_CDSUO: \" + pm.globals.get(\"NR_CDSUO\"))",
							"    console.log(\"NR_IDNSIP: \" + pm.globals.get(\"NR_IDNSIP\"))",
							"    console.log(\"entita.tipoAppartenenza: \" + entita.tipoAppartenenza)",
							"  }",
							"})",
							"",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							""
						],
						"type": "text/javascript"
