		{
			"name": "Ricerca ID_USER e ID_PERSONA by userId",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "0e66c1d7-a35f-434c-b54e-993e199fca71",
						"exec": [
							"var data = JSON.parse(responseBody);",
							"pm.globals.set(\"ID_USER\", Number(data.id));",
							"console.log(\"ID_USER: \" + pm.globals.get(\"ID_USER\"))",
							"",
							"pm.globals.set(\"ID_PERSONA\", Number(data.persona.id));",
							"console.log(\"ID_PERSONA: \" + pm.globals.get(\"ID_PERSONA\"))",
							"",
							"",
							"pm.test(\"UserName non presente\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"username\");",
							"});",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{ACCESS_TOKEN}}"
					}
				],
				"url": {
