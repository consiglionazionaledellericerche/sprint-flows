  <process id="MACCISS-raccoltaProposte" name="MACCISS-raccoltaProposte" isExecutable="true">
    <userTask id="inserisciProposte" name="PROPOSTE" activiti:candidateGroups="${assegna}" activiti:dueDate="PT${giorniScadenza}M">
      <extensionElements>
        <activiti:taskListener event="assignment" delegateExpression="${manageParallelTask_v1}">
          <activiti:field name="faseEsecuzione">
            <activiti:string><![CDATA[task-assignement]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${manageParallelTask_v1}">
          <activiti:field name="faseEsecuzione">
            <activiti:string><![CDATA[task-create]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" delegateExpression="${manageParallelTask_v1}">
          <activiti:field name="faseEsecuzione">
            <activiti:string><![CDATA[task-complete]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${assigneeList}" activiti:elementVariable="assegna"></multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="creaEnvironment" name="CREAZIONE" activiti:candidateGroups="AdminMACCISS">
      <extensionElements>
        <activiti:executionListener event="end" delegateExpression="${manageProcessParallel_v1}">
          <activiti:field name="faseEsecuzione">
            <activiti:string><![CDATA[process-prepareVariableNextTask]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </userTask>
    <endEvent id="End"></endEvent>
    <startEvent id="sid-9E7632D5-51B5-4E1E-B5FA-587AF55080EF" name="Start"></startEvent>
    <userTask id="assegnaFondi" name="FONDI" activiti:candidateGroups="DRUE"></userTask>
